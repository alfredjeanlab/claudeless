# Capture the setup/onboarding flow for new users.
# Run with: CLAUDE_CONFIG_DIR=$(mktemp -d) capsh --frames /tmp/out -- claude < setup-flow.capsh

wait "Welcome"
snapshot "setup_welcome"

wait 500
snapshot "setup_auth_prompt"

# Can't proceed further without actual authentication
send <C-c>
wait 200
send <C-c>
wait 200
kill TERM
